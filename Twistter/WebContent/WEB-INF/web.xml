<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd"> 
<web-app> 
	<display-name>Twister</display-name> 
	<servlet> 
		<servlet-name>createUser</servlet-name> 
		<servlet-class>servlets.user.CreateUser</servlet-class> 
	</servlet> 
	<servlet> 
		<servlet-name>addFriend</servlet-name> 
		<servlet-class>servlets.friend.AddFriend</servlet-class> 
	</servlet>
	<servlet> 
		<servlet-name>removeFriend</servlet-name> 
		<servlet-class>servlets.friend.RemoveFriend</servlet-class> 
	</servlet>
	<servlet> 
		<servlet-name>logout</servlet-name> 
		<servlet-class>servlets.user.Logout</servlet-class> 
	</servlet>
	<servlet> 
		<servlet-name>listFriend</servlet-name> 
		<servlet-class>servlets.friend.ListFriend</servlet-class> 
	</servlet>
	<servlet> 
		<servlet-name>login</servlet-name> 
		<servlet-class>servlets.user.Login</servlet-class> 
	</servlet>
	<servlet> 
		<servlet-name>addMessage</servlet-name> 
		<servlet-class>servlets.message.AddMessage</servlet-class> 
	</servlet> 
	<servlet> 
		<servlet-name>deleteMessage</servlet-name> 
		<servlet-class>servlets.message.DeleteMessage</servlet-class> 
	</servlet> 
	<servlet> 
		<servlet-name>listMessage</servlet-name> 
		<servlet-class>servlets.message.ListMessage</servlet-class> 
	</servlet> 
	<servlet> 
		<servlet-name>addLike</servlet-name> 
		<servlet-class>servlets.message.addLike</servlet-class> 
	</servlet> 
	<servlet> 
		<servlet-name>listComments</servlet-name> 
		<servlet-class>servlets.message.ListComments</servlet-class> 
	</servlet>
	<servlet-mapping> 
		<servlet-name>createUser</servlet-name> 
		<url-pattern>/createUser</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>addFriend</servlet-name> 
		<url-pattern>/addFriend</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>removeFriend</servlet-name> 
		<url-pattern>/removeFriend</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>logout</servlet-name> 
		<url-pattern>/logout</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>listFriend</servlet-name> 
		<url-pattern>/listFriend</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>login</servlet-name> 
		<url-pattern>/login</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>addMessage</servlet-name> 
		<url-pattern>/addMessage</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>deleteMessage</servlet-name> 
		<url-pattern>/deleteMessage</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>listMessage</servlet-name> 
		<url-pattern>/listMessage</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>addLike</servlet-name> 
		<url-pattern>/addLike</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>listComments</servlet-name> 
		<url-pattern>/listComments</url-pattern> 
	</servlet-mapping> 
</web-app>
